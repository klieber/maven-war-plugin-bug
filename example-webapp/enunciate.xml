<enunciate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="http://enunciate.webcohesion.com/schemas/enunciate-2.0.0-M.4.xsd">

  <title>Example Services</title>

  <api-classes>
    <include pattern="org.example.ExampleWebService"/>
  </api-classes>

  <modules>
    <jackson disabled="true" />
    <jackson1 disabled="true" />
    <docs docsSubdir="docs" disableResourceLinks="true" />
    <c-xml-client disabled="true" />
    <csharp-xml-client disabled="true" />
    <java-xml-client disabled="true" />
    <java-json-client disabled="true" />
    <javascript-client disabled="true" />
    <gwt-json-overlay disabled="true" />
    <obj-c-xml-client disabled="true" />
    <php-xml-client disabled="true" />
    <php-json-client disabled="true" />
    <ruby-json-client disabled="true" />
    <idl disabled="true" />
    <swagger disabled="true" />
  </modules>

</enunciate>
